{
  "name": "axiosone",
  "version": "0.1.0",
  "description": "A simple package to consolidate all your axios request configs into callable functions",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vui-Chee/axiosone"
  },
  "author": {
    "name": "Vui Chee",
    "email": "vuicheesiew@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "server": "json-server -p 3001 --watch sandbox/db.json",
    "es5": "es-check es5 lib/*",
    "lint": "eslint lib --ext js",
    "test": "jasmine --config=jasmine.json",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier -w lib test sandbox index.d.ts index.js"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.19.0",
    "es-check": "^5.2.4",
    "eslint": "^7.30.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "jasmine": "^3.8.0",
    "json-server": "^0.16.3",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "eslintConfig": {
    "globals": {
      "console": true,
      "module": true,
      "require": true
    },
    "root": true,
    "env": {
      "jasmine": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "jasmine"
    ]
  }
}
